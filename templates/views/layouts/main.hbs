<script type="text/javascript">
$(function(){
      // Instantiate MixItUp:
      $('#project-list').mixItUp({
        load: {
          sort: 'myorder:asc'
        }
      });
      // animate scroll scrollspy
      $('a[href^="#"]').click(function(){
          $('html, body').animate({
              scrollTop: $( $.attr(this, 'href') ).offset().top-50
          }, 500);
          return false;
      });
    });
</script>
